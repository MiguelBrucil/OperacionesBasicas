        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.321" />

    <EditText
        android:id="@+id/num1Multi"
        android:layout_width="119dp"
        android:layout_height="64dp"
        android:ems="10"
